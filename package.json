{
  "name": "delete-packages",
  "private": true,
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "build": "npm run format-check && npm run lint && npm run test && tsc",
    "pack": "rm -rf ./lib ./dist && npm run build && ncc build"
  },

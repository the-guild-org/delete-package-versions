    "url": "git+https://github.com/the-guild-org/delete-package-versions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.1.1",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
